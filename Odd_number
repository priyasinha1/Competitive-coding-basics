#include<iostream>

using namespace std;

int main()  {
    int N;
    cin>>N;
    // YOUR CODE GOES HERE
    for(int i = 1; i < N; i++){
        if (i%2!=0){
           cout<<i<<endl; 
        }
    } 
    
    return 0;
}

// Or

#include<iostream>

using namespace std;

int main()  {
    int N;
    cin>>N;
    // YOUR CODE GOES HERE
    for (int i = 1; i < N; i=i+2) {
        cout << i << "\n";
    }
   
    return 0;
}
